#!/usr/bin/env perl
use 5.010;
use Getopt::Std;
use Algorithm::Permute;
use Term::ANSIColor qw(:constants);

my @array = map { int(rand(2147483648)) } ( 1..2000 );
say join(" ", @array);
exit(0);

system "rm cases";
$i = 2;
while ($i <= 7)
{
	my @array = 1..$i;
	my $p_iterator = Algorithm::Permute->new ( \@array );
	while (my @perm = $p_iterator->next) {
		system "echo @perm >> cases";
	}
	$i = $i + 1;
	system "echo >> cases";
}
